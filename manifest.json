{
	"manifest_version": 3,
	"name": "Image Replacer",
	"description": "Replace all images with whatever you want.",
	"version": "1.1",
	"icons": {
	  "16": "icon16.png",
	  "48": "icon48.png",
	  "128": "icon128.png"
	},
	"action": {
	  "default_popup": "index.html",
	  "default_icon": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	  }
	},
	"permissions": ["activeTab", "storage"],
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["scripts/Main.js"]
	  }
	],
	"homepage_url": "https://i.kym-cdn.com/photos/images/newsfeed/002/069/850/076.jpg"
  }
  